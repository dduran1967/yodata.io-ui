{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "/widget",
        "destination": "/widget.html"
      },
      {
        "source": "/signup",
        "destination": "/signup.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
